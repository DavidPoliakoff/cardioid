simulate SIMULATE 
{
   anatomy = pioAnatomy;
   decomposition = koradi;
   diffusion = Salheen98Fast;
   reaction = tenTusscher04;
}

pioAnatomy ANATOMY 
{
   method = pio;
   fileName = snapshot.initial/anatomy#;
   cellTypes = -1;
   dx = 0.000167; // in meters
   dy = 0.000167; // in meters
   dz = 0.000167; // in meters
}

koradi DECOMPOSITION 
{
   method = koradi;
   verbose = 1;
   nCenters = 40;
   alpha = 0.5;
   maxVoronoiSteps = 100;
   maxSteps = 500;
   tolerance = 0.01;
   outputRate = 1;
   nbrDeltaR = 2;
}

grid DECOMPOSITION 
{
    method = grid;
    nx = 16;
    ny = 16;
    nz = 32;
}

Salheen98Fast DIFFUSION
{
   method = Salheen98;
   variant = precompute;
   conductivity = conductivity;
}

conductivity CONDUCTIVITY
{
    sigmaLi = 0.3;   // units S/m
    sigmaTi = 0.03;  // units S/m
}

tenTusscher04 REACTION
{
    method = tenTusscher04;
}
